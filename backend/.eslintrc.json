{
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "module",
        "parserOptions": {
            "ecmaVersion": 2018
        },
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-curly-brace-presence": "off",
        "arrow-parens": "off"
    }
}